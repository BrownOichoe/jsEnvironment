  },
  "homepage": "https://github.com/BrownOichoe/jsEnvironment#readme",
  "dependencies": {
    "whatwg-fetch": "^3.6.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.5",
    "@babel/node": "^7.15.4",
    "@babel/preset-env": "^7.15.6",
    "@babel/register": "^7.15.3",
    "babel-loader": "^8.2.2",
    "chai": "^4.3.4",
    "chalk": "^4.1.2",
    "cheerio": "^1.0.0-rc.10",
