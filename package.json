    "jsdom": "^17.0.0",
    "json-schema-faker": "^0.5.0-rcv.39",
    "json-server": "^0.16.3",
    "localtunnel": "^2.0.2",
    "mini-css-extract-plugin": "^2.3.0",
    "mocha": "^9.1.2",
    "npm-run-all": "^4.1.5",
    "numeral": "^2.0.6",
    "open": "^8.2.1",
    "rimraf": "^3.0.2",
